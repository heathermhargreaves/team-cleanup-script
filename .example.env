# Eppo API Configuration
# Copy this file to .env and fill in your actual values

# Your Eppo API authentication token (required)
EPPO_API_KEY=your_eppo_api_key_here

# Base URL for Eppo API (optional, defaults to https://eppo.cloud/api/v1)
EPPO_BASE_URL=https://eppo.cloud/api/v1

# Team ID to filter experiments by (required)
# Find your team ID by running the script first to see all available team IDs
TEAM_ID=your_team_id_here